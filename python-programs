# Execute the fibonacci series of a number:
a = 0
b = 1
n = int(input("Enter the series to be generated:"))
if n <= 0:
   print(f'Not valid')
elif n == 1:
    print(a)
elif n >= 2:
    print(a)
    print(b)
    for i in range(2, n):
        c = a+b
        a = b
        b = c
        print(c)
        
        
# Execute the fibonacci series using a function:
def fib(n):
    a = 0
    b = 1
    if n == 1:
        print(a)
    else:
        print(a)
        print(b)
        for i in range(2 , n):
            c = a+b
            a = b
            b = c
            print(c)
x=int(input("Enter a value:"))
fib(x)
